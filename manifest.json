{
  "manifest_version": 3,
  "name": "F1 Dashboard - New Tab",
  "version": "1.0.0",
  "description": "Transform your new tab into a stunning F1 dashboard with live stats, standings, and race schedules",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api.openf1.org/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
